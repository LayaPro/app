{
  "name": "api-services",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "build:dev": "tsc -p tsconfig.dev.json",
