    "@aws-sdk/client-ses": "^3.962.0",
    "@types/node-cron": "^3.0.11",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exifr": "^7.1.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^7.6.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "p-map": "^7.0.2",
    "qrcode": "^1.5.3",
